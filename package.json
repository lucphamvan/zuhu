        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^6.0.0",
        "jest": "^29.3.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.16",
        "supertest": "^6.3.2",
        "swagger-autogen": "^2.22.0",
        "swagger-ui-express": "^4.6.0"
    }
