{
    "name": "nodejs-server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest --detectOpenHandles",
        "start": "npm run db:init && npm run build && cross-env NODE_PATH=./dist node ./dist/index.js",
        "db:migrate": "prisma generate",
        "db:push": "prisma db push",
        "db:reset": "prisma migrate reset --force",
        "db:init": "npx prisma migrate dev --name init",
        "build": "tsc --build --clean && tsc",
        "watch": "tsc --watch",
        "dev": "cross-env NODE_PATH=./dist nodemon ./dist/index.js",
        "start:dev": "npm run build && concurrently \"npm run watch\" \"npm run dev\"",
        "swagger-autogen": "node swagger.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^29.2.4",
        "@types/lodash": "^4.14.182",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.45",
        "@types/supertest": "^2.0.12",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "express": "^4.18.1",
        "namor": "^2.0.3",
        "prisma": "^4.7.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.7.2",
        "uuid": "^8.3.2"
    },
    "dependencies": {
        "@prisma/client": "^4.7.1",
        "bcrypt": "^5.0.1",
